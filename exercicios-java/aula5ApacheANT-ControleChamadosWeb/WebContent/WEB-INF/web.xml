<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>chamadoWeb</display-name>
	<servlet>
		<servlet-name>ConsultarChamado</servlet-name>
		<display-name>ConsultarChamado</display-name>
		<description></description>
		<servlet-class>br.edu.fiap.web.ConsultarChamado</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CadastrarChamado</servlet-name>
		<display-name>CadastrarChamado</display-name>
		<description></description>
		<servlet-class>br.edu.fiap.web.CadastrarChamado</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConsultarChamado</servlet-name>
		<url-pattern>/ConsultarChamado</url-pattern>
