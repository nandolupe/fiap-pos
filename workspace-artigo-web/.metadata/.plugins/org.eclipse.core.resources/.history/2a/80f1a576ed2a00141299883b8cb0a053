    <!-- Identifica o Managed Bean a ser gerenciado pelo JSF -->
	<managed-bean>
		<!-- Nome do Managed Bean -->
		<managed-bean-name>clienteMB</managed-bean-name>
		<!-- Classe a ser gerenciada pelo JSF como um Managed Bean -->
		<managed-bean-class>com.fiap.teste.bean.ClienteMB</managed-bean-class>
		<!-- Tempo de vida do Managed Bean, nesse caso Session. -->
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

